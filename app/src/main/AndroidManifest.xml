<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="surfer.example.surfer.robobore">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Corsit" />
        <activity android:name=".RoboCore" />
        <activity android:name=".Events" />
        <activity android:name=".Team" />
        <activity android:name=".Sponsor" />
        <activity android:name=".ContactUs" />
        <activity android:name=".Location" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".WelcomeActivity" />
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".Hackathon" />
        <activity android:name=".Robotics" />
        <activity android:name=".StudentRegistartion" />
        <activity android:name=".LineFollower" />
        <activity android:name=".CrossRoads" />
        <activity android:name=".RoboRash" />
        <activity android:name=".PaperPresenation" />
        <activity android:name=".RoboSoccer" />
        <activity android:name=".Dcode"></activity>
    </application>

</manifest>